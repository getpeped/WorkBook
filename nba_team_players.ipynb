   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "nx, ny = (500, 500)\n",
    "x = np.linspace(-3, 3, nx)\n",
    "y = np.linspace(-3, 3, ny)\n",
    "\n",
    "xx, yy = np.meshgrid(x, y)\n",
    "z = xx ** 2 + yy ** 2\n",
    "\n",
    "plt.contourf(xx, yy, z, cmap='viridis')"
   ]
